{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://notesadda.vercel.app" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
}
